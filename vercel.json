{
  "version": 2,
  "builds": [
    {
      "src": "client/src/index.js",
      "use":"@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "client/src/index.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin":"https://www.ivancamassa.com/"
      }
    }
  ]
}
